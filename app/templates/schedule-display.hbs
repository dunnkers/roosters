<table class="schedule">
<thead>
<tr>
<th>Uur\<wbr>Dag</th>
<th>maandag</th>
<th>dinsdag</th>
<th>woensdag</th>
<th>donderdag</th>
<th>vrijdag</th>
</tr>
</thead>

<tbody>

{{#each row in rows}}
  <tr>
  {{#each cell in row}}
    <td>
      {{#if cell.header}}
          u{{cell.index}}
      {{else}}
          {{cell-display lessons=cell.content}}
      {{/if}}
    </td>
  {{/each}}
  </tr>
{{/each}}

</tbody>
</table>
