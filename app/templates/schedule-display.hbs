<table class="schedule">
<thead>
<tr>
<th>Uur\<wbr>Dag</th>
<th>maandag</th>
<th>dinsdag</th>
<th>woensdag</th>
<th>donderdag</th>
<th>vrijdag</th>
</tr>
</thead>

<tbody>

{{#each row in rows}}
  <tr>
  {{#each day in row}}
    <td>
      {{cell-display lessons=day.content}}
    </td>
  {{/each}}
  </tr>
{{/each}}

</tbody>
</table>
